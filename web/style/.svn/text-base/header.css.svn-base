#header-wrapper {

}

#header {
    clear: both;
    width: 100%;
    margin: 0px 0px;
    background: rgb(30,87,153); /* Old browsers */
    background: -moz-linear-gradient(top,  rgba(30,87,153,1) 0%, rgba(38,117,201,1) 60%, rgba(32,124,202,1) 94%, rgba(125,185,232,1) 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(30,87,153,1)), color-stop(60%,rgba(38,117,201,1)), color-stop(94%,rgba(32,124,202,1)), color-stop(100%,rgba(125,185,232,1))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top,  rgba(30,87,153,1) 0%,rgba(38,117,201,1) 60%,rgba(32,124,202,1) 94%,rgba(125,185,232,1) 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,  rgba(30,87,153,1) 0%,rgba(38,117,201,1) 60%,rgba(32,124,202,1) 94%,rgba(125,185,232,1) 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top,  rgba(30,87,153,1) 0%,rgba(38,117,201,1) 60%,rgba(32,124,202,1) 94%,rgba(125,185,232,1) 100%); /* IE10+ */
    background: linear-gradient(to bottom,  rgba(30,87,153,1) 0%,rgba(38,117,201,1) 60%,rgba(32,124,202,1) 94%,rgba(125,185,232,1) 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#1e5799', endColorstr='#7db9e8',GradientType=0 ); /* IE6-9 */

    padding: 0px;
    border-radius:6px
}

#logo {
    width: 100%;
    height: 60px;
    margin: 0px;
    padding: 0px 0px;
    color: white;
}

#logo h1, #logo p {
    margin: 0;
    padding: 0;
}

#logo h1 {
    margin: 0px;
    padding: 0px;
    text-transform: capitalize;
    font-size: 2.0em;
    color: #323030;
}

